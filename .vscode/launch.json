    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug process_region",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/process_region.py",
            "args": [],
            "env": {
                "REGION_PATH": "north-america/us/district-of-columbia",
                "S3_BUCKET": "nix",
            }
        },
        {
            "name": "Pyflyte Debug: process_single_region",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/pyflyte",
            "args": [
                "run",
                "osm_pipeline.py",
                "process_single_region",
                "--region_path",
                "north-america/us/utah",
                "--pbf_file",
                "${workspaceFolder}/data/north-america_us_utah-latest.osm.pbf",
                "--"
            ],
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.venv/bin/python",
            "env": {
                "PGHOST": "localhost",
                "PGPORT": "5432",
                "PGDATABASE": "osm",
                "PGUSER": "mvexel",
                "PGPASSWORD": ""
            }
        },
        {
            "name": "Pyflyte Debug: 4 states parallel",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/pyflyte",
            "args": [
                "run",
                "osm_pipeline.py",
                "osm_poi_pipeline_multi",
                "--region_paths",
                "[\"north-america/us/utah\",\"north-america/us/colorado\",\"north-america/us/new-mexico\",\"north-america/us/arizona\"]",
                "--"
            ],
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.venv/bin/python",
            "env": {
                "PGHOST": "localhost",
                "PGPORT": "5432",
                "PGDATABASE": "osm",
                "PGUSER": "mvexel",
                "PGPASSWORD": ""
            }
        },
        {
            "name": "Pyflyte Remote: 4 states parallel",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/pyflyte",
            "args": [
                "run",
                "--remote",
                "osm_pipeline.py",
                "osm_poi_pipeline_multi",
                "--region_paths",
                "[\"north-america/us/utah\",\"north-america/us/colorado\",\"north-america/us/new-mexico\",\"north-america/us/arizona\"]",
                "--"
            ],
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/.venv/bin/python"
        }
    ]
}